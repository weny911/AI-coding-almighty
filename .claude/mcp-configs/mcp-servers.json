{
  "mcpServers": {
    "mysql": {
      "command": "npx",
      "args": [
        "universal-db-mcp",
        "--type",
        "mysql",
        "--host",
        "<MYSQL__HOST>",
        "--port",
        "<MYSQL__PORT>",
        "--user",
        "<MYSQL__USER>",
        "--password",
        "<MYSQL__PASSWORD>",
        "--database",
        "<MYSQL__DATABASE>"
      ]
    },
    "redis-cache": {
      "command": "npx",
      "args": [
        "universal-db-mcp",
        "--type",
        "redis",
        "--host",
        "<REDIS_HOST>",
        "--port",
        "<REDIS_PORT>",
        "--password",
        "<REDIS_PASSWORD>",
        "--database",
        "<REDIS_DATABASE>",
        "--danger-allow-write"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {
        "UPSTASH_REDIS_REST_URL": "<UPSTASH_REDIS_REST_URL>",
        "UPSTASH_REDIS_REST_TOKEN": "<UPSTASH_REDIS_REST_TOKEN>"
      }
    },
    "nanobanana": {
      "command": "uvx",
      "args": [
        "nanobanana-mcp-server@latest"
      ],
      "env": {
        "GEMINI_API_KEY": "<GEMINI_API_KEY>"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "spec-workflow": {
      "command": "npx",
      "args": [
        "-y",
        "@pimzino/spec-workflow-mcp@latest",
        "<SPEC_WORKFLOW_PROJECT_PATH>"
      ]
    },
    "Figma": {
      "url": "https://mcp.figma.com/mcp",
      "headers": {}
    },
    "pencil": {
      "name": "pencil",
      "transport": "stdio",
      "command": "<PENCIL_MCP_BINARY_PATH>",
      "args": [
        "--app",
        "cursor"
      ],
      "env": {}
    }
  }
}
