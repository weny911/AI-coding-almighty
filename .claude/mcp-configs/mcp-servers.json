{
  "mcpServers": {
    "mysql-local": {
      "command": "npx",
      "args": [
        "-y",
        "@neigezhujiayi/mysql-mcp-server",
        "--host",
        "sh-cdb-s1m3xqps.sql.tencentcdb.com",
        "--port",
        "25531",
        "--user",
        "kaimen_develop",
        "--password",
        "your-password-here",
        "--database",
        "kaimen_refactor_test"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "nanobanana": {
      "command": "uvx",
      "args": ["nanobanana-mcp-server@latest"],
      "env": {
        "GEMINI_API_KEY": "your_gemini_api_key_here"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@executeautomation/playwright-mcp-server"]
    },
    "postgres": {
      "command": "npx",
        "args": [
            "-y",
            "@modelcontextprotocol/server-postgres",
            "postgresql://postgres:postgres@host:5432/postgres"
      ]
    },
    "spec-workflow": {
      "command": "npx",
      "args": ["-y", "@pimzino/spec-workflow-mcp@latest", "/your/project/path"]
    }
  }
}